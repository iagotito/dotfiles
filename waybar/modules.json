{
  // Workspaces
  "hyprland/workspaces" : {
    "on-click": "activate",
    "active-only": false,
    "all-outputs": true,
    "format": "{icon}",
    "format-icons": {
    "active": "",
    "default": "",
    "empty": ""
    },
    "persistent-workspaces": {
       "*": 2
    }
  },

  // Taskbar
  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 18,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
       "Alacritty"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },

  // Hyprland Window
  "hyprland/window": {
    "rewrite": {
       "(.*) - Mozilla Firefox": "üåé $1"
    },
    "separate-outputs": true
  },

  // Cliphist
  "custom/cliphist": {
    "format": "ÔÉ™",
    "on-click": "sleep 0.1 && ~/dotfiles/scripts/cliphist.sh",
    "on-click-right": "sleep 0.1 && ~/dotfiles/scripts/cliphist.sh d",
    "on-click-middle": "sleep 0.1 && ~/dotfiles/scripts/cliphist.sh w",
    "tooltip": false
  },

  // Hyprshade
  "custom/hyprshade": {
    "format": "Óìú",
    "on-click": "sleep 0.1; ~/dotfiles/hypr/scripts/hyprshade.sh",
    "on-click-right": "sleep 0.1; ~/dotfiles/hypr/scripts/hyprshade.sh rofi",
    "tooltip": false
  },

  // Hypridle inhibitor
  "custom/hypridle": {
    "format": "ÔÄ£",
    "return-type": "json",
    "escape": true,
    "exec-on-event": true,
    "interval": 60,
    "exec": "~/dotfiles/hypr/scripts/hypridle.sh status",
    "on-click": "~/dotfiles/hypr/scripts/hypridle.sh toggle",
    "on-click-right": "hyprlock"
  },

  // Updates Count
  "custom/updates": {
    "format": "ÔÇ´   {}",
    "tooltip-format": "{}",
    "escape": true,
    "return-type": "json",
    "exec": "~/dotfiles/scripts/updates.sh",
    "restart-interval": 60,
    "on-click": "alacritty -e ~/dotfiles/scripts/installupdates.sh",
    "tooltip": false
  },

  // Theme Starter Label
  "custom/starter": {
    "format": "THEME STARTER",
    "tooltip": false
  },

  // Wallpaper
  "custom/wallpaper": {
    "format": "ÔÄæ",
    "on-click": "~/dotfiles/hypr/scripts/wallpaper.sh select",
    "on-click-right": "~/dotfiles/hypr/scripts/wallpaper-effects.sh",
    "tooltip": false
  },

  // Waybar Themes
  "custom/waybarthemes": {
    "format": "ÔÖÅ",
    "on-click": "~/dotfiles/waybar/themeswitcher.sh",
    "tooltip": false
  },

  // Settings
  "custom/settings": {
    "format": "ÔÄì",
    "on-click": "~/dotfiles/apps/ML4W_Dotfiles_Settings-x86_64.AppImage",
    "tooltip": false
  },

  // Keybindings
  "custom/keybindings": {
    "format": "ÔÑú",
    "on-click": "~/dotfiles/hypr/scripts/keybindings.sh",
    "tooltip": false
  },

  // Filemanager Launcher
  "custom/filemanager": {
    "format": "ÔÅª",
    "on-click": "thunar",
    "tooltip": false
  },

  // Browser Launcher
  "custom/browser": {
    "format": "Ôãê",
    "on-click": "~/dotfiles/.settings/browser.sh",
    "tooltip": false
  },

  // ML4W Welcome App
  "custom/ml4w-welcome": {
    "on-click": "~/dotfiles/apps/ML4W_Welcome-x86_64.AppImage",
    "format": " ",
    "tooltip": false
  },

  // ChatGPT Launcher
  "custom/chatgpt": {
    "format": "ÔÅô",
    "on-click": "chromium --app=https://chat.openai.com",
    "tooltip": false
  },

  // Rofi Application Launcher
  "custom/appmenu": {
    "format": "ÔÄï",
    "on-click": "rofi -show drun -replace",
    //"on-click-right": "~/dotfiles/hypr/scripts/keybindings.sh",
    "tooltip": false
  },

  // Power Menu
  "custom/exit": {
    "format": "ÔÄë",
    "on-click": "wlogout",
    "tooltip": false
  },

  // Keyboard State
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name} {icon}",
    "format-icons": {
      "locked": "ÔÄ£",
      "unlocked": "ÔÇú"
    }
  },

  // System tray
  "tray": {
    // "icon-size": 21,
    "spacing": 10
  },

  // Clock
  "clock": {
    // "timezone": "America/New_York",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "{:%Y-%m-%d}"
  },

  // System
  "custom/system": {
    "format": "Óë≥",
    "tooltip": false
  },

  // CPU
  "cpu": {
    "format": "/ C {usage}% ",
    "on-click": "alacritty -e htop"
  },

  // Memory
  "memory": {
    "format": "/ M {}% ",
    "on-click": "alacritty -e htop"
  },

  // Harddisc space used
  "disk": {
    "interval": 30,
    "format": "D {percentage_used}% ",
    "path": "/",
    "on-click": "alacritty -e htop"
  },

  "hyprland/language": {
    "format": "/ K {short}"
  },

  // Group Hardware
  "group/hardware": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 300,
      "children-class": "not-memory",
      "transition-left-to-right": false
    },
    "modules": [
      "custom/system",
      "disk",
      "cpu",
      "memory",
      "hyprland/language"
    ]
  },

  // Group Settings
  "group/settings": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 300,
      "children-class": "not-memory",
      "transition-left-to-right": false
    },
    "modules": [
      "custom/settings",
      "custom/waybarthemes",
      "custom/wallpaper"
    ]
  },

  // Group Quicklinks
  "group/quicklinks": {
    "orientation": "horizontal",
    "modules": [
      "custom/filemanager",
      "custom/browser"
     ]
  },

  // Network
  "network": {
    "format": "{ifname}",
    "format-wifi": "Ôá´   {signalStrength}%",
    "format-ethernet": "Ôûñ  {ipaddr}",
    "format-disconnected": "Not connected", //An empty format will hide the module.
    "tooltip-format": "Ôûñ {ifname} via {gwaddri}",
    "tooltip-format-wifi": "Ôá´   {essid} ({signalStrength}%)",
    "tooltip-format-ethernet": "ÔÉÅ  {ifname} ({ipaddr}/{cidr})",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50,
    "on-click": "alacritty -e nmtui"
  },

  // Battery
  "battery": {
    "states": {
      // "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{icon}   {capacity}%",
    "format-charging": "Ôóß  {capacity}%",
    "format-plugged": "Ôá¶  {capacity}%",
    "format-alt": "{icon}  {time}",
    // "format-good": "", // An empty format will hide the module
    // "format-full": "",
    "format-icons": ["ÔâÑ ", "ÔâÉ ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "]
  },

  // Pulseaudio
  "pulseaudio": {
    // "scroll-step": 1, // %, can be a float
    "format": "{icon}  {volume}%",
    "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
    "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
    "format-muted": "Ôö© {format_source}",
    "format-source": "{volume}% ÔÑ∞",
    "format-source-muted": "ÔÑ±",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Ôñê",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄ® ", "ÔÄ® "]
    },
    "on-click": "pavucontrol"
  },

  // Bluetooth
  "bluetooth": {
    "format-disabled": "",
    "format-off": "",
    "interval": 30,
    "on-click": "blueman-manager",
    "format-no-controller": ""
  },

  // Other
  "user": {
    "format": "{user}",
    "interval": 60,
    "icon": false,
  },

  // Idle Inhibator
  "idle_inhibitor": {
    "format": "{icon}",
    "tooltip": true,
    "format-icons":{
      "activated": "ÔèÅ",
      "deactivated": "ÔÄ£"
    },
    "on-click-right": "hyprlock"
  }
}
